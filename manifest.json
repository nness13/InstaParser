{
  "name": "InstaJinn",
  "description": "Расширить стандартные возможности инстаграм и даст преимущества в использовании",
  "version": "0.0.31",
    "icons": {
        "16": "assets/images/16x16.png",
        "48": "assets/images/48x48.png",
        "128": "assets/images/128x128.png"
    },   
  "permissions": [
    "tabs", "https://www.instagram.com/*","storage"
  ],
  "browser_action": {
      "default_title": "InstaJinn",
      "default_icon": "assets/images/icon.png",
      "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["build.js","bg.js"]
  },
  "manifest_version": 2
}
